<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">

	<acl>
		<resources>
			<resource id="Magento_Backend::admin">

				<resource id="Roweb_Blog::Blog" title="Blog" sortOrder="10">
					<resource id="Roweb_Blog::Blog_save" title="Save Blog" sortOrder="10"/>
					<resource id="Roweb_Blog::Blog_delete" title="Delete Blog" sortOrder="20"/>
					<resource id="Roweb_Blog::Blog_update" title="Update Blog" sortOrder="30"/>
					<resource id="Roweb_Blog::Blog_view" title="View Blog" sortOrder="40"/>
				</resource>

				<resource id="Magento_Backend::stores">
					<resource id="Magento_Backend::stores_settings">
						<resource id="Magento_Config::config">
							<resource id="Roweb_Blog::config_roweb_blog" title="Blog"/>
						</resource>
					</resource>
				</resource>

			</resource>
		</resources>
	</acl>

</config>
